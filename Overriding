class Vehicle:
    def describe(self):
        print('This is a genetic vehicle.')
        
class Car(Vehicle):
    print('This is a sporty car.')
    
class Motorcycle(Vehicle):
    def describe(self):
        print('This is a fast motorcycle.')
        
vehicle = Vehicle()
car = Car()
motorcycle = Motorcycle()

#objects
vehicle.describe()
car.describe()
motorcycle.describe()
